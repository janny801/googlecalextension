{
  "manifest_version": 3,
  "name": "AI Day Planner",
  "version": "1.0",
  "description": "A Chrome extension that uses AI to plan your day on Google Calendar.",
  "permissions": ["identity", "storage"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/bronny.png",
      "48": "icons/bronny.png",
      "128": "icons/bronny.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "oauth2": {
    "client_id": "61213083646-rnv7d7n06qdvbbdjpff3hps1dfr4m5le.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "icons": {
    "16": "icons/bronny.png",
    "48": "icons/bronny.png",
    "128": "icons/bronny.png"
  }
}
