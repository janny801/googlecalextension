{
    "manifest_version": 3,
    "name": "AI Day Planner",
    "version": "1.0",
    "description": "A Chrome extension that uses AI to plan your day on Google Calendar.",
    "permissions": ["identity", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "oauth2": {
      "client_id": "YOUR_GOOGLE_CLIENT_ID", 
      "scopes": [
        "https://www.googleapis.com/auth/calendar.events"
      ]
    }
  }
  